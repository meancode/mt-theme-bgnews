<mt:If tag="EntryAuthorDisplayName">
	<mt:If tag="EntryAuthorDisplayName" like="(Associated Press|The BG News|The College News Network)">
        By <span class="vcard author"><$mt:EntryAuthorDisplayName$>
<mt:Else>
        By <span class="vcard author"><a href="/profile/<mt:EntryAuthorUsername>"><$mt:EntryAuthorDisplayName$></a><mt:If tag="authordatabyline_position">, <mt:authordatabyline_position></mt:If>
        </mt:If>
</mt:If>
<div class="asset-meta">
<mt:If tag="EntryCategory">
<mt:IfArchiveType type="individual">
<div style="float:right;">
<mt:IfCategory label="City" type="primary">
