</mt:IfArchiveType>
</mt:If>
    <span class="byline">
        <abbr class="published" title="<$mt:EntryDate format="%b %d, %Y %I:%M %p"$>">Published: <$mt:EntryDate format="%B %d, %Y"$><mt:If tag="entrydataupdated"> (Updated: <mt:entrydataupdated format="%b %d, %Y">) </mt:If></abbr>
    </span>
<mt:Unless name="hide_counts" eq="1">
    <mt:IfCommentsActive><a href="<$mt:EntryPermalink$>#comments" class="entry-meta-comments" title="<$mt:EntryCommentCount singular="1 Comment" plural="# Comments"$>"><$mt:EntryCommentCount$></a></mt:IfCommentsActive>
    <a href="javascript:void(0)" title="Vote/Favorite this article" onclick="return mtScore(<$mt:EntryID$>)" id="scoring-id-<$mt:EntryID$>" class="scoring">
