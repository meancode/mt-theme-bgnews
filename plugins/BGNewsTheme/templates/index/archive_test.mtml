<div id="bgnews-home-page">            
<div class="page-asset asset">
  <div class="asset-header">
    <h1 id="page-title" class="asset-name"><$mt:BlogName encode_html="1"$> Archives</h1>
  </div>
  <div class="asset-content">
    <div class="asset-body">
	
<p>This page contains the daily and monthly archives of The BG News. We are in the process of getting our back catalog of issues and author archives online.  </p>

<div style="width: 630px;">
	
<mt:IfArchiveTypeEnabled archive_type="Daily">
  <div class="archive-category archive">
      <h2 class="archive-header">Issue Index</h2>
    <mt:ArchiveList archive_type="Daily" lastn="1">
      <mt:ArchiveListHeader>
      <div class="archive-content" style="width:200px; float:left; clear: none; padding-right:20px;">
        <div class="widget-calendar widget" style="height:220px;">
          <h3 class="widget-header">
            <mt:Calendar month="this"><mt:If tag="CalendarDay" eq="1"><mt:CalendarDate format="%B %Y"></mt:If></mt:Calendar>
          </h3>
          <div class="widget-content">
        </mt:ArchiveListHeader>
            <table align="center" cellspacing="7" summary="Issue index">
              <tr>
                <th abbr="Sunday">S</th>
                <th abbr="Monday">M</th>
                <th abbr="Tuesday">T</th>
                <th abbr="Wednesday">W</th>
                <th abbr="Thursday">T</th>
                <th abbr="Friday">F</th>
                <th abbr="Saturday">S</th>
              </tr>
              <MTCalendar month="this">
                <MTCalendarWeekHeader>
                  <tr>
                </MTCalendarWeekHeader>
                <td align="center"><MTCalendarIfEntries> <a href="<$mt:ArchiveLink type="Daily"$>" class="daily-highlight" style="display: block;">
                      <$MTCalendarDay$>
                      </a> </MTCalendarIfEntries>
                    <MTCalendarIfNoEntries>
                      <$MTCalendarDay$>
                    </MTCalendarIfNoEntries>
                    <MTCalendarIfBlank> </MTCalendarIfBlank></td>
                  <MTCalendarWeekFooter>
                </tr></MTCalendarWeekFooter>
              </MTCalendar>
            </table>
        <mt:ArchiveListFooter>
          </div>
        </div>
      </div>
      </mt:ArchiveListFooter>
    </mt:ArchiveList>

    <mt:ArchiveList archive_type="Daily" lastn="1">
      <mt:ArchiveListHeader>
      <div class="archive-content" style="width:200px; float:left; clear: none; margin-bottom:0px;">
        <div class="widget-calendar widget" style="height:220px;">
          <h3 class="widget-header">
            <mt:Calendar month="last"><mt:If tag="CalendarDay" eq="1"><mt:CalendarDate format="%B %Y"></mt:If></mt:Calendar>
          </h3>
          <div class="widget-content">
        </mt:ArchiveListHeader>
            <table align="center" cellspacing="7" summary="Issue index">
              <tr>
                <th abbr="Sunday">S</th>
                <th abbr="Monday">M</th>
                <th abbr="Tuesday">T</th>
                <th abbr="Wednesday">W</th>
                <th abbr="Thursday">T</th>
                <th abbr="Friday">F</th>
                <th abbr="Saturday">S</th>
              </tr>
              <MTCalendar month="last">
                <MTCalendarWeekHeader>
                  <tr>
                </MTCalendarWeekHeader>
                <td align="center"><MTCalendarIfEntries> <a href="<$mt:ArchiveLink type="Daily"$>" class="daily-highlight" style="display: block;">
                      <$MTCalendarDay$>
                      </a> </MTCalendarIfEntries>
                    <MTCalendarIfNoEntries>
                      <$MTCalendarDay$>
                    </MTCalendarIfNoEntries>
                    <MTCalendarIfBlank> </MTCalendarIfBlank></td>
                  <MTCalendarWeekFooter>
                </tr></MTCalendarWeekFooter>
              </MTCalendar>
            </table>
        <mt:ArchiveListFooter>
          </div>
        </div>
<p align="right"><a href="http://bgnews.com/issue/">View Previous Issues &raquo;</a></p>
      </div>
      </mt:ArchiveListFooter>
    </mt:ArchiveList>

  </div>  
</mt:IfArchiveTypeEnabled>	
</div><!-- // END Issue Index Block -->

<div style="width: 630px;">
<div class="archive-category archive">
  <h2 class="archive-header">Monthly Index</h2>
  <div class="archive-content">

<table id="archives-table">
<mt:ArchiveList type="Yearly" sort_order="ascend">
    <mt:ArchiveListHeader><tr><$mt:ArchiveDate format="%Y" setvar="start_year"$></mt:ArchiveListHeader>
    <th><$mt:ArchiveDate format="%Y"$></th>
    <$mt:ArchiveDate format="%Y" setvar="archive_year"$>
    <$mt:SetVar name="is_posts_year_{$archive_year}" value="1"$>
    <mt:ArchiveList type="Monthly">
        <$mt:ArchiveDate format="%m_%Y" setvar="month_year"$>
        <mt:SetVarBlock name="links_{$month_year}"><a href="<$mt:ArchiveLink$>"><$mt:ArchiveDate format="%b"$></a></mt:SetVarBlock>
    </mt:ArchiveList>
    <mt:ArchiveListFooter></tr><$mt:ArchiveDate format="%Y" setvar="end_year"$></mt:ArchiveListFooter>
</mt:ArchiveList>
<mt:For var="month" from="1" to="12">
    <tr>
    <mt:For var="year" from="$start_year" to="$end_year">
        <mt:If name="is_posts_year_{$year}">
            <mt:SetVarBlock name="month_year"><$mt:Var name="month" sprintf="%02d"$>_<$mt:Var name="year"$></mt:SetVarBlock>
        <td><$mt:Var name="links_{$month_year}" default="&nbsp;"$></td>
        </mt:If>
    </mt:For>
    </tr>
</mt:For>
</table>
  </div>
</div>
</div><!-- // END Monthly Archive Block -->

